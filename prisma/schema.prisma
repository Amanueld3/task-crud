generator client {
  provider = "prisma-client-js"
  seed     = "npm run seed"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id             String     @id @default(uuid())
  name      String
  username       String     @unique
  hashedPassword String
  createdAt      DateTime   @default(now())
  updatedAt      DateTime   @updatedAt()
}
